@model IEnumerable<KurumsalWeb.Models.Model.Slider>
  
<div id="main-slider" class="flexslider">
    <ul class="slides">
        @foreach (var item in Model)
        {
            <li>
                <img src="@item.ResimUrl" alt="@item.Baslik" />
                <div class="flex-caption">
                    <h3>@item.Baslik</h3>
                    <p>@item.Aciklama</p>
                </div>
            </li>
        }
    </ul>
</div>
